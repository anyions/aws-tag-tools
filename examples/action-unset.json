{
  "action": "unset",
  "partition": "aws",
  "regions": [
    "ap-southeast-1",
    "ap-northeast-1"
  ],
  "tags": [
    "owner",
    "tags[?value == 'awstt'].key"
  ],
  "resources": [
    "ec2:instance",
    "arn:aws:rds:*:*:db:mysql-instance",
    {
      "target": "arn:aws:ec2:*:*:vpc/*",
      "tags": [
        "tagged_at"
      ],
      "filter": "tags[?key == 'others']"
    },
    {
      "target": "arn:aws:ec2:*:*:volume/*",
      "tags": [
        "tags[?key=='owner' && value == 'aws-tag-tools'].key"
      ],
      "filter": "tags[?key == 'tagged_at' && value < '${now().strftime('%Y-%m-%d %H:%M:%S')}$']"
    }
  ]
}
