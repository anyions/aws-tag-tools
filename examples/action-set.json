{
  "action": "set",
  "partition": "aws",
  "regions": [
    "ap-southeast-1",
    "ap-northeast-1"
  ],
  "tags": [
    {
      "key": "tagged_by",
      "value": "awstt"
    },
    {
      "key": "tagged_at",
      "value": "${datetime.now().strftime('%Y-%m-%d %H:%M:%S')}$"
    }
  ],
  "resources": [
    "ec2:ebs",
    "arn:aws:rds:*:*:db:mysql-instance",
    {
      "target": "arn:aws:ec2:*:*:vpc/*",
      "tags": [
        {
          "key": "tagged_by",
          "value": "AnyIons"
        }
      ]
    },
    {
      "target": "arn:aws:ec2:*:*:instance/*",
      "tags": [
        {
          "key": "owner",
          "value": "AnyIons"
        }
      ],
      "force": true,
      "filter": "!(tags[?key == 'others'])"
    }
  ],
  "force": false
}
